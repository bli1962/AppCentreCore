<InputSelect id="DurationUnit" class="form-control" disabled=@IsEdit placeholder="Duration Unit " @bind-Value="@ViewModel.DurationUnit">
    @foreach (var durationunit in DurationUnit)
    {
        <option value="@durationunit">@durationunit</option>
    }
</InputSelect>

@code {
    [Parameter]
    public BisRateViewModel ViewModel { get; set; }

    [Parameter]
    public BisRateDurationUnit[] DurationUnit { get; set; }

    [Parameter]
    public bool IsEdit { get; set; }

}
